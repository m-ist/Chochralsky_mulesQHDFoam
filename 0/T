FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 1511;

boundaryField
{
    outerCylinder
    {
        type	       zeroGradient;
    }

    disk
    {
        type	       fixedValue; 
        value          uniform 1500;
    }
    outlet
    {
        type	       fixedValue;
        value          uniform 1511;
    }
}


